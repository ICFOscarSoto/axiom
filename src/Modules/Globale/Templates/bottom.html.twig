{% block javascript %}
	<!-- Bottom scripts (common) -->
	<script type="text/javascript">
		jQuery(document).ready(function($)
		{
				//Checkboxes transformations
				$('input.icheck').iCheck({
					checkboxClass: 'icheckbox_flat-blue',
					radioClass: 'iradio_flat-blue'
				});
		});
	</script>
	<script src="{{ url('index') }}js/gsap/TweenMax.min.js"></script>
	<script src="{{ url('index') }}js/joinable.js"></script>
	<script src="{{ url('index') }}js/resizeable.js"></script>
	<script src="{{ url('index') }}js/toastr.js"></script>
	<script src="{{ url('index') }}js/neon-api.js"></script>
	<script src="{{ url('index') }}js/jquery.validate.min.js"></script>
	<script src="{{ url('index') }}js/jquery.validate_es.js"></script>
	<script src="{{ url('index') }}js/neon-chat.js"></script>
	<script src="{{ url('index') }}js/jquery-ui/js/jquery-ui-1.10.3.minimal.min.js"></script>
	<script src="{{ url('index') }}js/bootstrap.min.js"></script>
	<script src="{{ url('index') }}js/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
	<script src="{{ url('index') }}js/jquery-confirm/jquery-confirm.min.js"></script>

	<!-- JavaScripts initializations and stuff -->
	<script src="{{ url('index') }}js/globale/notifications.js"></script>
	<script src="{{ url('index') }}js/globale/emails.js"></script>
	<script src="{{ url('index') }}js/globale/dates.js"></script>
	<script src="{{ url('index') }}js/neon-custom.js"></script>
	<script src="{{ url('index') }}js/select2/select2.min.js"></script>
	<script src="{{ url('index') }}js/icheck/icheck.min.js"></script>
	<script src="{{ url('index') }}js/bootstrap-tagsinput.min.js"></script>
	<script src="{{ url('index') }}js/moment-duration-format.min.js"></script>
	<script src="{{ url('index') }}js/raphael-min.js"></script>
	<script src="{{ url('index') }}js/morris.min.js"></script>
	<script src="{{ url('index') }}js/chartjs/Chart.min.js"></script>
	<script src="{{ url('index') }}js/colpick.js"></script>
	{% if include_footer is defined %}
		{% for item in include_footer %}
			{% if item.type=="css" %}
				<link rel="stylesheet" href="{{ url('index') }}{{ item.path }}">
			{% endif %}
			{% if item.type=="js" %}
				<script src="{{ url('index') }}{{ item.path }}"></script>
			{% endif %}
		{% endfor %}
	{% endif %}
{% endblock %}
