
{% set params = app.request.attributes.get('_route_params')|merge(app.request.query.all()) %}
{% block header %}
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<meta name="description" content="Neon Admin Panel" />
	<meta name="author" content="" />
	<link rel="icon" href="images/favicon.ico">
	{{ include('header.html.twig') }}
	<!-- Imported styles on this page -->
	<link rel="stylesheet" href="{{ url('index') }}/js/jvectormap/jquery-jvectormap-1.2.2.css">
	<link rel="stylesheet" href="{{ url('index') }}/js/rickshaw/rickshaw.min.css">
{% endblock %}
<title>{% block title %}{{ interfaceName }}{% endblock %}</title>

<body class="page-body" data-url="{{ url('indexAdmin') }}">
<div class="page-container"><!-- add class "sidebar-collapsed" to close sidebar by default, "chat-visible" to make chat appear always -->
	{{ include('sidebar_menu.html.twig') }}
	<div class="main-content">
		{{ include('head.html.twig') }}
		<hr />
		{{ include('breadcrumb.html.twig') }}
		<!-- MAIN CONTENT -->
		{% if (formData is not null) and (formData.section is not null)%}
			{% for section in formData.section %}
			<div class="row">
				<div class="col-md-12">
					<div class="panel panel-primary">
						<div class="panel-heading">
							<div class="panel-title">
									{{section.label}}
							</div>
						</div>
						<div class="panel-body">
				{% for row in section.rows  %}
					{% for field in row %}
					<div class="form-group">
						{% if field.type =="checkbox" %}
						<div class="col-{% if field.size is not null %}{{field.size}}{% else %}{{section.size}}{% endif %}">
									<label for="{{ field.id }}" class=" control-label">{{ field.name}}</label>
									<div class="make-switch">
											<input type="checkbox" checked="">
									</div>
						</div>
						{% elseif field.type =="textarea" %}
						<div class="col-{% if field.size is not null %}{{field.size}}{% else %}{{section.size}}{% endif %}">
									<label for="{{ field.id }}" class=" control-label">{{ field.name}}</label>
									<textarea type="{{field.type}}" class="form-control autogrow" id="{{ field.id }}" placeholder="Placeholder" {% if (field.disabled is defined) and (field.disabled==true)%}disabled=""{% endif %}></textarea>
						</div>

						{% elseif field.type =="select" %}
						<div class="col-{% if field.size is not null %}{{field.size}}{% else %}{{section.size}}{% endif %}">
									<label for="{{ field.id }}" class=" control-label">{{ field.name}}</label>
									<select type="{{field.type}}" class="form-control" id="{{ field.id }}" {% if (field.disabled is defined) and (field.disabled==true)%}disabled=""{% endif %}></select>
						</div>
						{% elseif field.type =="datepicker" %}
						<div class="col-{% if field.size is not null %}{{field.size}}{% else %}{{section.size}}{% endif %}">
									<label for="{{ field.id }}" class=" control-label">{{ field.name}}</label>
									<input type="text" class="form-control datepicker" id="{{ field.id }}" {% if (field.disabled is defined) and (field.disabled==true)%}disabled=""{% endif %} DataFormat="{{ field.dataformat }}"></select>
						</div>
						{% else %}
						<div class="col-{% if field.size is not null %}{{field.size}}{% else %}{{section.size}}{% endif %}">
									<label for="{{ field.id }}" class=" control-label">{{ field.name}}</label>
									<input type="{{field.type}}" class="form-control" id="{{ field.id }}" placeholder="Placeholder" {% if (field.disabled is defined) and (field.disabled==true)%}disabled=""{% endif %}>
						</div>
						{% endif %}
						</div>
					{% endfor %}
				{% endfor %}
					</div>
				</div>
			</div>
		</div>
			{% endfor %}
		{% endif %}


		<h2>{% trans %} Company {% endtrans %}</h2>
		<div class="row">
			<div class="col-md-12">
				<div class="panel panel-primary" data-collapsed="0">
					<div class="panel-heading">
						<div class="panel-tittle">
							{% trans %} General Data {% endtrans %}
						</div>
					</div>
					<div class="panel-body">
						<form class="form-horizontal form-groups-bordered" role="form">
							 <div class="form-group">
							 	<div class="row">
								 	<div class="col-md-6">
								 		<label for="field-1" class=" control-label">{% trans %} Name {% endtrans %}</label>
								 		<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
								 	</div>
								 	<div class="col-md-3">
									 	<label for="field-1" class="control-label">{% trans %} Social Name {% endtrans %}</label>
									 	<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
								 	</div>
								 	<div class="col-md-3">
									 	<label for="field-1" class="control-label">{% trans %} Vat {% endtrans %}</label>
									 	<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
								 	</div>
								</div>
								<div class="row">
									<div class="col-md-4">
									 	<label for="field-1" class="control-label">{% trans %} Country {% endtrans %}</label>
								 		<select class="form-control" id="field-1"></select>
								 	</div>
								 	<div class="col-md-4">
								 		<label for="field-1" class="control-label">{% trans %} State {% endtrans %}</label>
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>
							 		<div class="col-md-4">
							 			<label for="field-1" class="control-label">{% trans %} City {% endtrans %}</label>
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>
							 	</div>
							 	<div class="row">
							 		<div class="col-md-3">
							 			<label for="field-1" class="control-label">{% trans %} Postcode {% endtrans %}</label>
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>
							 		<div class="col-md-4">
							 			<label for="field-1" class="control-label">{% trans %} Address {% endtrans %}</label>
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>
							 	</div>
							 	<div class="row">
							 		<div class="col-md-4">
							 			<label for="field-1" class="control-label">{% trans %} Phone {% endtrans %}</label>
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>
							 		<div class="col-md-4">
							 			<label for="field-1" class="control-label">{% trans %} Mobile {% endtrans %}</label>
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>

							 	</div>
							 </div>
						</form>
					</div>
				</div>
				<div class="panel panel-primary" data-collapsed="0">
					<div class="panel-heading">
						<div class="panel-tittle">
							{% trans %} Company Option {% endtrans %}
						</div>
					</div>
					<div class="panel-body">
						<form class="form-horizontal form-groups-bordered" role="form">
							 <div class="form-group">
							 	<div id="label-switch" class="make-switch" data-on-label="<i class='entypo-check'></i>" data-off-label="<i class='entypo-cancel'></i>">
										<input type="checkbox" checked>
								</div>
								<div id="label-switch" class="make-switch" data-on-label="<i class='entypo-check'></i>" data-off-label="<i class='entypo-cancel'></i>">
										<input type="checkbox" checked>
								</div>
							 </div>
						</form>
					</div>
				</div>
			</div>
		</div>


		<!-- END MAIN CONTENT -->
		{{ include('footer.html.twig') }}
	</div>
	{{ include('chat.html.twig') }}
	<!-- Chat Histories -->
	{{ include('chat_histories.html.twig') }}
</div>
	<!-- MODALS  -->
	<!-- END MODALS -->
{% block footer %}
    {{ include('bottom.html.twig') }}
	<link rel="stylesheet" href="{{ url('index') }}css/font-icons/font-awesome/css/font-awesome.min.css">
	<!-- Imported scripts on this page -->
	<script src="{{ url('index') }}js/jvectormap/jquery-jvectormap-europe-merc-en.js"></script>
	<script src="{{ url('index') }}js/jquery.sparkline.min.js"></script>
	<script src="{{ url('index') }}js/rickshaw/vendor/d3.v3.js"></script>
	<script src="{{ url('index') }}js/rickshaw/rickshaw.min.js"></script>
	<script src="{{ url('index') }}js/raphael-min.js"></script>
	<script src="{{ url('index') }}js/morris.min.js"></script>
	<script src="{{ url('index') }}js/toastr.js"></script>
	<script src="{{ url('index') }}js/neon-chat.js"></script>
	<script src="{{ url('index') }}js/bootstrap-switch.min.js"></script>
	<script src="{{ url('index') }}js/bootstrap-datepicker.js"></script>
{% endblock %}
</body>
