{% set params = app.request.attributes.get('_route_params')|merge(app.request.query.all()) %}
{% block header %}
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<meta name="description" content="Neon Admin Panel" />
	<meta name="author" content="" />
	<link rel="icon" href="images/favicon.ico">
	{{ include('header.html.twig') }}
	<!-- Imported styles on this page -->

{% endblock %}
<title>{% block title %}{{ interfaceName }}{% endblock %}</title>
<body class="page-body" data-url="{{ url('indexAdmin') }}">
<div class="page-container"><!-- add class "sidebar-collapsed" to close sidebar by default, "chat-visible" to make chat appear always -->
	{{ include('sidebar_menu.html.twig') }}
	<div class="main-content">	
		{{ include('head.html.twig') }}	
		<hr />

		{{ include('breadcrumb.html.twig') }}
		<!-- MAIN CONTENT -->	
		{{ form_start(form) }}
		{{ form_widget(form) }}
		{{ form_end(form) }}
		<h2>{% trans %} Company {% endtrans %}</h2>
		<div class="row">
			<div class="col-md-12">
				<div class="panel panel-primary" data-collapsed="0">
					<div class="panel-heading">
						<div class="panel-tittle">
							{% trans %} General Data {% endtrans %}
						</div>
					</div>
					<div class="panel-body">
						<form class="form-horizontal form-groups-bordered" role="form">							
							 <div class="form-group"> 
							 	<div class="row">
								 	<div class="col-md-6">
								 		<label for="field-1" class=" control-label">{% trans %} Name {% endtrans %}</label> 		 	
								 		<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >				 	
								 	</div>
								 	<div class="col-md-3">
									 	<label for="field-1" class="control-label">{% trans %} Social Name {% endtrans %}</label> 
									 	<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
								 	</div> 
								 	<div class="col-md-3">
									 	<label for="field-1" class="control-label">{% trans %} Vat {% endtrans %}</label> 				 	 
									 	<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
								 	</div> 
								</div>
								<div class="row">
									<div class="col-md-4">
									 	<label for="field-1" class="control-label">{% trans %} Country {% endtrans %}</label>  	
								 		<select class="form-control" id="field-1"></select>							 
								 	</div> 
								 	<div class="col-md-4">
								 		<label for="field-1" class="control-label">{% trans %} State {% endtrans %}</label> 
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>
							 		<div class="col-md-4">
							 			<label for="field-1" class="control-label">{% trans %} City {% endtrans %}</label>  	
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div> 
							 	</div>
							 	<div class="row">
							 		<div class="col-md-3">
							 			<label for="field-1" class="control-label">{% trans %} Postcode {% endtrans %}</label> 
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>
							 		<div class="col-md-4">
							 			<label for="field-1" class="control-label">{% trans %} Address {% endtrans %}</label> 
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>
							 	</div>
							 	<div class="row">
							 		<div class="col-md-4">
							 			<label for="field-1" class="control-label">{% trans %} Phone {% endtrans %}</label>  
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div>
							 		<div class="col-md-4">
							 			<label for="field-1" class="control-label">{% trans %} Mobile {% endtrans %}</label> 
							 			<input type="text" class="form-control" id="field-1" placeholder="Placeholder" >
							 		</div> 
							 		
							 	</div>
							 </div>							 
						</form>
					</div>
				</div>
				<div class="panel panel-primary" data-collapsed="0">
					<div class="panel-heading">
						<div class="panel-tittle">
							{% trans %} General Data {% endtrans %}
						</div>
					</div>
					<div class="panel-body">
<form role="form" class="form-horizontal form-groups-bordered">					
<div class="form-group">
		<div class="col-sm-offset-3 col-sm-5">
			<div class="checkbox">
				<label>
					<input type="checkbox">Checkbox 1
				</label>
			</div>
</div>
</div>
						<form role="form" class="form-horizontal form-groups-bordered">
			
							<div class="form-group">
								<label class="col-sm-3 control-label">Toggle Check</label>
								
								<div class="col-sm-5">
									<input type="checkbox" data-toggle="toggle" checked="">
									
								</div>
							</div>
					</div>
				</div>
			</div>
			</div>
		</div>
<!-- END MAIN CONTENT -->	
		{{ include('footer.html.twig') }}
	</div>
	{{ include('chat.html.twig') }}
	<!-- Chat Histories -->
	{{ include('chat_histories.html.twig') }}
</div>
	<!-- MODALS  -->
	<!-- END MODALS -->
{% block footer %}
    {{ include('bottom.html.twig') }}
<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
<script src="https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js"></script>

<script src="{{ url('index') }}js/gsap/TweenMax.min.js"></script>
<script src="{{ url('index') }}js/bootstrap-switch.min.js"></script>
<script src="{{ url('index') }}js/neon-chat.js"></script>

	<!-- Bottom scripts (common) -->
	<script src="{{ url('index') }}js/gsap/TweenMax.min.js"></script>
	<script src="{{ url('index') }}js/jquery-ui/js/jquery-ui-1.10.3.minimal.min.js"></script>
	<script src="{{ url('index') }}js/bootstrap.js"></script>
	<script src="{{ url('index') }}js/joinable.js"></script>
	<script src="{{ url('index') }}js/resizeable.js"></script>
	<script src="{{ url('index') }}js/neon-api.js"></script>


	<!-- Imported scripts on this page -->
	<script src="{{ url('index') }}js/bootstrap-switch.min.js"></script>
	<script src="{{ url('index') }}js/neon-chat.js"></script>


	<!-- JavaScripts initializations and stuff -->
	<script src="{{ url('index') }}js/neon-custom.js"></script>


	<!-- Demo Settings -->
	<script src="{{ url('index') }}js/neon-demo.js"></script>

{% endblock %}
</body>
